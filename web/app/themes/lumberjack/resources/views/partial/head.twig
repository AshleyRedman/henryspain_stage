<head>
    <meta charset="{{ site.charset }}" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <!-- Icon Meta -->
    <link rel="apple-touch-icon" sizes="180x180" href="{{ theme.link }}/assets/meta/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ theme.link  }}/assets/meta/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ theme.link  }}/assets/meta/favicon-16x16.png">
    <link rel="manifest" href="{{ theme.link  }}/assets/meta/site.webmanifest">
    <link rel="mask-icon" href="{{ theme.link  }}/assets/meta/safari-pinned-tab.svg" color="#ffffff">
    <link rel="shortcut icon" href="{{ theme.link  }}/assets/meta/favicon.ico">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-config" content="{{ theme.link  }}/assets/meta/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">

    <title>{{ function('wp_title') }}{% if function('is_front_page') %}{{ site.title }}{% endif %} - {{ site.description }}</title>

    {{ function('wp_head') }}

</head>
